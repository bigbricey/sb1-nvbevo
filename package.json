{
  "name": "jax-sod",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:analyze": "ANALYZE=true npm run build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "generate-sitemap": "node scripts/generate-sitemap.js",
    "generate-image-sitemap": "npm install glob && node scripts/generate-image-sitemap.js",
    "generate-favicons": "npm install canvas && node scripts/generate-favicons.js",
    "validate-schema": "node scripts/validate-structured-data.js",
    "check-links": "npm install node-fetch cheerio && node scripts/check-broken-links.js",
    "analyze-seo": "npm install glob && node scripts/analyze-content-seo.js",
    "prebuild": "npm run generate-sitemap"
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.5.10",
    "@types/react-helmet-async": "^1.0.1",
    "canvas": "^3.1.0",
    "cheerio": "^1.0.0",
    "glob": "^11.0.1",
    "lucide-react": "^0.344.0",
    "node-fetch": "^3.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-ga": "^3.3.1",
    "react-ga4": "^2.1.0",
    "react-helmet-async": "^2.0.5",
    "react-intersection-observer": "^9.15.1",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^6.22.3",
    "react-simple-chatbot": "^0.5.0"
  },
  "devDependencies": {
    "@types/node": "^20.17.19",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.18",
    "compression-webpack-plugin": "^11.1.0",
    "eslint": "^8.56.0",
    "imagemin-webp": "^8.0.0",
    "postcss": "^8.4.35",
    "rollup-plugin-visualizer": "^5.14.0",
    "sharp": "^0.33.5",
    "tailwindcss": "^3.4.1",
    "terser": "^5.39.0",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vite-plugin-imagemin": "^0.6.1"
  }
}
